	function get_number_icon($k) {
	global $mobile_icons;
		$ua = null;

		if ( preg_match('/DoCoMo\//', $_SERVER['HTTP_USER_AGENT'], $m) ) {
			return $mobile_icons['docomo']["Xnum$k"];
		} elseif ( preg_match('/WILLCOM/', $_SERVER['HTTP_USER_AGENT'], $m) ) {
			return $mobile_icons['docomo']["Xnum$k"];
		} elseif ( preg_match('/KDDI-/', $_SERVER['HTTP_USER_AGENT'], $m) ) {
			$key = "Xnum$k";
			return $mobile_icons['au'][$key];
		} elseif ( preg_match('/^(MOT-|Vodafone|J-PHONE|SoftBank)/', $_SERVER['HTTP_USER_AGENT'], $m) ) {
			return $mobile_icons['voda']["Xnum$k"];
		} else {
			return '[' . ((string)$k) . ']';
		}
	}

